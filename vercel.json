{
    "version": 2,
    "builds": [
        {
            "src": "src/main.ts",
            "use": "@vercel/node"
        }
    ],
    "headers": [
        {
          "source": "/api/(.*)",
          "headers": [
            { "key": "Access-Control-Allow-Origin", "value": "https://mshl.lat" },
            { "key": "Access-Control-Allow-Methods", "value": "GET,POST" }
          ]
        }
      ]
}